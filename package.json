{
  "name": "gemini",
  "version": "4.5.62",
  "description": "A dope, easy-on-the-eyes, currently-playing viewer for Spotify.",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dev": "nodemon --delay 1 --watch app --exec electron ."
  },
  "author": "Gabe Haarberg <gabe@knightlightshow.com>, Kyle Awayan <kyle@awayan.com",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.0.3",
    "electron-builder": "^23.0.3",
    "nodemon": "^2.0.15",
    "prettier": "2.6.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "electron-settings": "^4.0.2",
    "electron-updater": "^4.3.4",
    "electron-window-state": "^5.0.3",
    "express": "^4.17.2",
    "jquery": "^3.6.0",
    "node-fetch": "2.6.7",
    "spotify-web-api-node": "^5.0.2"
  },
  "build": {
    "appId": "io.kylan.gemini",
    "productName": "Gemini",
    "copyright": "Copyright Â© 2022 Gabe and Kyle",
    "files": [
      "!.env"
    ],
    "linux": {
      "publish": [
        "github"
      ]
    },
    "directories": {
      "output": "build/builds"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gabe-H/Gemini.git"
  },
  "bugs": {
    "url": "https://github.com/Gabe-H/Gemini/issues"
  },
  "homepage": "https://github.com/Gabe-H/Gemini#readme"
}
